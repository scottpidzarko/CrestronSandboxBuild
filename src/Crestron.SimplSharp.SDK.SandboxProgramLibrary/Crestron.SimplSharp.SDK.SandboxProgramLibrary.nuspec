<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <!-- Required elements-->
        <id>Crestron.SimplSharp.SDK.SandboxProgramLibrary</id>
        <version>2.19.16-beta1</version>
        <description>A version of Crestron.SimplSharp.SDK.ProgramLibrary suitable for use with a 3-series sandbox build. See https://github.com/scottpidzarko/CrestronSandboxBuild
		This project pulls the required files from the Device Database. You MUST have the S# plugin and everything it requires installed for this package to function.</description>
        <authors>Scott Pidzarko</authors>
        <!-- Optional elements -->
		<title>Crestron Sandbox S# ProgramLibrary</title>
		<copyright>Copyright (C) 2022 to the present CrestronSandboxBuild Contributors.</copyright>
		<releaseNotes>
			Initial release. Visual Studio 2019 and Jetbrains Rider tested with successful builds. Visual Studio 2022 is not supported at this time, but that is planned for a future release.
		</releaseNotes>
        <projectUrl>https://github.com/scottpidzarko/CrestronSandboxBuild</projectUrl>
		<license type="expression">MIT</license>
		<tags>Crestron Sandbox 3-series SIMPL# SimplSharp SIMPL#Pro S# compactframework</tags>
		<repository type="git" url="https://github.com/scottpidzarko/CrestronSandboxBuild" branch="main" />
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<dependencies>		
			<dependency id="Crestron.SimplSharp.SDK.SandboxLibrary" version="2.19.16-beta1"/>
			<!-- we just need the targets and build from this package, the lib folder isn't useful in this case so exclude it -->
			<dependency id="Crestron.SimplSharp.SDK.ProgramLibrary" version="2.19.16" />
		</dependencies>
		<frameworkAssemblies>
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.AudioDistribution" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.CrestronConnected" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.DeviceSupport" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.DM" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.EthernetCommunications" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.Fusion" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.Gateways" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.GeneralIO" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.Keypads" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.Lighting" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.Media" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.Remotes" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.Shades" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.Thermostats" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.ThreeSeriesCards" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.UC" />
			<frameworkAssembly assemblyName="Crestron.SimplSharpPro.UI" />
			<frameworkAssembly assemblyName="SimplSharpPro" />
		</frameworkAssemblies>		
    </metadata>	
</package>